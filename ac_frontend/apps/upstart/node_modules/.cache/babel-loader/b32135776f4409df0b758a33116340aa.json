ner>\r\n    </v-card>\r\n</template>\r\n\r\n<script>\r\n\r\n\r\nexport default {\r\n  name: \"ApiCard\",\r\n  components: {},\r\n  props: [\r\n    'apiContent'\r\n  ],\r\n  data() {\r\n    return {\r\n      endpoint: \"\",\r\n      port: \"\",\r\n      protocol: \"\"\r\n    }\r\n  },\r\n  computed: {\r\n    fullEndpoint() {\r\n      return this.protocol + \"//\" + this.endpoint + (this.port != '80' && this.port != '' ? \":\" + this.port : \"\") + this.apiContent.path\r\n    },\r\n    projectId() {\r\n      return \"project-\" + window.location.href.split(\"project-\")[1].split(\"/api\")[0]\r\n    }\r\n  },\r\n  methods: {\r\n    getLabel(method) {\r\n      switch (method) {\r\n        case 'POST':\r\n          return 'green'\r\n        case 'DELETE':\r\n          return 'red'\r\n        case 'PUT':\r\n          return 'blue'\r\n        default:\r\n          return 'grey'\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.endpoint = window.location.hostname\r\n    this.port = window.location.port\r\n    this.protocol = window.location.protocol\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.require-font{\r\n  font-style: italic;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}