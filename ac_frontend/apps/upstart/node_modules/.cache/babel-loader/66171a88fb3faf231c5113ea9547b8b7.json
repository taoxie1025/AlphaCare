panel = []\r\n    },\r\n    onCancel() {\r\n      this.collapseAll()\r\n    },\r\n    onAppendComment(comment) {\r\n      this.$emit('appendComment', comment)\r\n    }\r\n  },\r\n  mounted() {\r\n    bus.$on('creatingNewTicket', this.onCreatingNewTicket)\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/components"}]}