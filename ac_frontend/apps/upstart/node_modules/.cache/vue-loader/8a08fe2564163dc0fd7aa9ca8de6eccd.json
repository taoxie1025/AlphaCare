apseAll() {\r\n      this.panel = []\r\n    },\r\n    expandCollapse() {\r\n      if (!this.isExpandAll) {\r\n        this.expandALl()\r\n        this.isExpandAll = true\r\n      } else {\r\n        this.collapseAll()\r\n        this.isExpandAll = false\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#button-wrap {\r\n  display: inline;\r\n}\r\n#panel {\r\n}\r\n</style>"]}]}